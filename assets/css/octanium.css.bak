@media (min-width: 992px) and (max-width: 1399px) {
  .octa-banner .block2 .banner-image .block2-img1 {
    width: 550px;
    height: auto;
  }
}

@media (min-width: 768px) and (max-width: 1149px) {
  .octa-banner .block1 h1 {
    margin-right: -172px;
  }
}
